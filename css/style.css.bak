#main-navbar{
  background-color: blue;
  overflow:hidden;
}

#navbar-container{
  float:left;
  width: 100%;
  position: relative;
}

#navbar-container ul{
  list-style-type: none;
  margin: 0;
  padding: 0;
  position: relative;
  left: 50%;
  float: left;
}

#navbar-container ul li{
  float: left;
  position: relative;
  right: 50%;
  display: block;
}

#navbar-container ul li a:hover{
  background-color: #000;
}

#onpage{
  text-decoration: underline overline;
}

